   "outputs": [],
   "source": [
    "def read_companies_list(file_name):\n",
    "    companies = [] # list of company tickers on MOEX\n",
    "    \n",
    "    with open(file_name) as f:\n",
    "        f_csv = csv.reader(f, delimiter = ';')\n",
    "        headers = next(f_csv)\n",
    "        for row in f_csv:\n",
    "            if row[12] == 'RPMO': #skip all stocks used in REPO only\n",
    "                pass \n",
    "            else: \n",
    "                companies = companies + [row[0]]\n",
    "                    \n",
    "    #Clean first entries\n",
    "    companies = companies[2:]\n",
    "    \n",
    "    return companies\n",
    "\n",
    "# **************************************************************#\n",
    "# TRANSFORM MOEX TICKERS INTO RICs FOR THOMSON REUTERS DATABASE:\n",
    "\n",
    "def create_RICs(companies):\n",
    "    RICs = []\n",
    "    for i, company in enumerate(companies):\n",
    "        if len(company) == 4:\n",
    "            RICs = RICs + [company+'.MM'] # for all 4-symbol tickers add '.MM'\n",
    "        else:\n",
    "            RICs = RICs + [company[:4]+'_p.MM']   # for all 5-symbol thickers delete last \"P\"\n",
    "    return RICs\n",
    "\n",
    "# **************************************************************#\n",
    "# CUMULATIVE FUNCTION TO GET MOEX TICKERS AND TRANSFORM THEM INTO RICS\n",
    "\n",
    "def get_companies(file_name):\n",
    "    companies = read_companies_list(file_name)\n",
    "    RICs = create_RICs(companies)\n",
    "    \n",
    "    return RICs\n",
    "\n",
    "def get_companies(file_name):\n",
    "    companies = read_companies_list(file_name)\n",
    "    RICs = create_RICs(companies)\n",
    "    \n",
    "    return RICs\n",
    "\n",
    "def get_esg_scores(companies):\n",
    "    fields = ['TR.TRESGScore', 'TR.TRESGResourceUseScore' , 'TR.TRESGEmissionsScore' 'TR.TRESGInnovationScore',\n",
    "              'TR.TRESGWorkforceScore', 'TR.TRESGHumanRightsScore', 'TR.TRESGCommunityScore', \n",
    "              'TR.TRESGProductResponsibilityScore', 'TR.TRESGManagementScore', 'TR.TRESGShareholdersScore', \n",
    "              'TR.TRESGCSRStrategyScore', 'TR.SocialPillarScore', 'TR.GovernancePillarScore', 'TR.EnvironmentPillarScore',\n",
    "              'TR.TRESGScore.date']\n",
    "    params = {'Period':'FY0', 'Frq' : 'FY', 'SDate' : '0', 'EDate' : '-8', 'CH' : 'Fd', 'RH' : 'IN', 'NULL': 'NA'}\n",
    "    ESGscores, e = ek.get_data(instruments = companies, fields = fields, parameters = params)\n",
    "    return ESGscores\n",
    "\n",
    "def get_variables(companies):\n",
    "    fields = ['TR.TotalLiabilities.date','TR.CompanyMarketCap','TR.TotalLongTermDebt','TR.TotalEquity','TR.TotalAssetsReported',\n",
    "              'TR.NetIncomeAfterTaxes','TR.OperatingIncome','TR.TotalLiabilities', 'TR.Revenue']\n",
    "    params = {'Period':'FY0', 'Frq' : 'FY', 'SDate' : '0', 'EDate' : '-8', 'Curn' : 'RUB', 'CH' : 'Fd', 'RH' : 'IN', 'NULL': 'NA'}\n",
    "    variables, e = ek.get_data(instruments = companies, fields = fields, parameters = params)\n",
    "    return variables\n",
    "\n",
    "def get_age(companies):\n",
    "    fields = ['TR.CompanyPublicSinceDate']\n",
    "    params = {'Period':'FY0', 'Frq' : 'FY', 'SDate' : '0', 'EDate' : '-8', 'CH' : 'Fd', 'RH' : 'IN', 'NULL': 'NA'}\n",
    "    variables, e = ek.get_data(instruments = companies, fields = fields, parameters = params)\n",
    "    return variables\n"
   ]
  },
