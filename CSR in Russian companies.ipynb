    "def get_companies(country):\n",
    "    syntax = 'SCREEN(U(IN(Equity(active,public,private,primary))/*UNV:PublicPrivate*/),  IN(TR.ExchangeName, \"'+country+'\"))'\n",
    "    fields = ['TR.CommonName', 'TR.PrimaryQuote', 'TR.HeadquartersCountry', 'TR.TRBCEconomicSector', 'TR.TRBCBusinessSector', 'TR.TRBCIndustryGroup', 'TR.TRBCIndustry']\n",
    "    companies, e = ek.get_data(syntax, fields)\n",
    "    return companies\n",
    "\n",
    "def get_variables(companies):\n",
    "    fields = ['TR.Revenue.date','TR.Revenue']\n",
    "    params = {'Period':'FQ0', 'Frq' : 'FQ', 'SDate' : '0', 'EDate' : '-40', 'Curn' : 'EUR', 'CH' : 'Fd', 'RH' : 'IN', 'NULL': 'NA'}\n",
    "    variables, e = ek.get_data(instruments = companies, fields = fields, parameters = params)\n",
    "    return variables"
   ]
